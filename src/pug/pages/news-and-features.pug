.call-out.call-out--long
  .call-out__image(style="background-image: url('/images/news-and-features/banner.jpg')")
    .call-out__image__content
      h1.h3-like.call-out__title News and Features
  .call-out__list-container.js-dropdown
    button.call-out__mobile-list.js-dropdown-button.for-tablet News and Features
    ul.call-out__list.js-dropdown-content
      li.is-active
        a(href="/news-and-features.html") News and Features
      li
        a(href="/press-releases.html") Press Releases
      li
        a(href="/resources.html") Useful Resources

